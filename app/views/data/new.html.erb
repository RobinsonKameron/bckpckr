<div class="col-md-4 col-md-offset-4">
    <div class="well">
            <%= form_for :entry do |f| %>
         <div class="form-group">
           <%= f.label :date %>
           <%= f.date_field :date, class:"form-control" %>
          </div>
          <div class="form-group">
            <%= f.label :budget %>
            <%= f.text_field  :budget, class:"form-control" %>
          </div>
          <div class="form-group">
            <%= f.label :destination %>
            <%= f.submit "Submit", class:"btn btn-default" %>
      <% end %>
    </div>
  </div>